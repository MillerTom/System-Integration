<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:p="http://primefaces.org/ui">
    <h:head>
        <title>Forget Password Result</title>
    </h:head>
    <h:body>
        <h:panelGroup id="messagePanel" layout="block">
            <h:messages globalOnly="true" errorStyle="color: red" infoStyle="color: green" layout="table"/>
        </h:panelGroup>
        <h:form id="form"> 
            <h3 align="center" style="color: green">Your New Password has been sent to Your Mail Successfully, Please Check Your Mail</h3>
            <br/><br/>
            <h:commandButton value="Back to Login Page" action="/login?faces-redirect=true" type="submit" />
        </h:form>
    </h:body>
</html>